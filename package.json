{
	"name": "tiny-project-api",
	"version": "1.1.0",
	"description": "a node express rest api for my various projects",
	"main": "src/app.ts",
	"scripts": {
		"start": "pm2-runtime start ecosystem.config.js --env production",
		"build": "shx rm -rf build/ && tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
		"dev": "ts-node-dev --respawn --transpile-only --exit-child --watch -r tsconfig-paths/register src/app.ts",
		"deploy": "git push heroku main"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/przekwas/tiny-project-api.git"
	},
	"keywords": [
		"js",
		"javascript",
		"typescript",
		"node",
		"express"
	],
	"author": "Luke Przekwas",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/przekwas/tiny-project-api/issues"
	},
	"homepage": "https://github.com/przekwas/tiny-project-api#readme",
	"dependencies": {
		"bcrypt": "^5.0.1",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"discord.js": "^13.6.0",
		"dotenv": "^16.0.0",
		"express": "^4.17.3",
		"express-validator": "^6.14.0",
		"helmet": "^5.0.2",
		"jsonwebtoken": "^8.5.1",
		"morgan": "^1.10.0",
		"mysql": "^2.18.1",
		"passport": "^0.5.2",
		"passport-jwt": "^4.0.0",
		"passport-local": "^1.0.0",
		"pm2": "^5.2.0",
		"uuid": "^8.3.2",
		"winston": "^3.6.0"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/compression": "^1.7.2",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/morgan": "^1.9.3",
		"@types/mysql": "^2.15.21",
		"@types/node": "^17.0.21",
		"@types/passport": "^1.0.7",
		"@types/passport-jwt": "^3.0.6",
		"@types/passport-local": "^1.0.34",
		"@types/uuid": "^8.3.4",
		"shx": "^0.3.4",
		"ts-node-dev": "^1.1.8",
		"tsc-alias": "^1.6.4",
		"tsconfig-paths": "^4.1.2",
		"typescript": "^4.6.2"
	}
}
